/*-------------------------------------------------------------------------*/

/**  HEADER  --------------------------------------------------------------*/
.header {
  font-size: 165%;
  letter-spacing: -1px;
  margin: 2% auto;
  text-transform: uppercase;
  width: 90%;
}
.header a:link, .header a:visited, .header a:hover, .header a:focus, .header a:active {
  background-color: #eee;
  border-bottom: none;
  color: #aaa;
  padding: 0;
  text-decoration: none;
}
.header a:hover, .header a:focus {
  background-color: inherit;
  color: #000;
}
.header h1 {
  background-color: inherit;
  color: #aaa;
  font-family: "DejaVuSans-ExtraLight", "DejaVu Sans", "Helvetica Neue", Arial, sans-serif;
  font-weight: 100;
  padding-left: 0.2em;
}
.header h1 em {
  font-weight: 900;
}
.header small {
  background-color: inherit;
  color: #fff;
  margin-left: -4px;
  vertical-align: sub;
}
/*-------------------------------------------------------------------------*/

/**  PAGE STRUCTURE  ------------------------------------------------------*/
#page {
  margin: 0 auto;
  width: 90%;
}
#page .section, #page .nav {
  float: right;
}
#page .section .section {
  float: none;
}
#content {
  width: 60%;
}
#contextual-content {
  margin-right: 2%;
  width: 23%;
  min-width: 23%;
}
